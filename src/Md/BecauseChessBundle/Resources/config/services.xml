<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="player_pool.class">Md\BecauseChessFileSystemBridge\FileSystemPlayerPool</parameter>
    </parameters>

    <services>
        <service id="player_pool" class="%player_pool.class%">
            <argument>%player_pool.file_path%</argument>
        </service>
    </services>
</container>
